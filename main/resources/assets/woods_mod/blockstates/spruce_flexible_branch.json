{
    "multipart": [
        {   "when": { "north": true },
            "apply": { "model": "woods_mod:block/spruce_flexible_branch_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "woods_mod:block/spruce_flexible_branch_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "woods_mod:block/spruce_flexible_branch_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "woods_mod:block/spruce_flexible_branch_side", "y": 270, "uvlock": true }
        },
        {   "when": { "up": true },
            "apply": { "model": "woods_mod:block/spruce_flexible_branch_side", "x": 270, "uvlock": true }
        },
        {   "when": { "down": true },
            "apply": { "model": "woods_mod:block/spruce_flexible_branch_side", "x": 90, "uvlock": true }
        },
        {   "when": { "north": false},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2 }
            ]
        },
        {   "when": { "south": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "y": 180, "uvlock": true }
            ]
        },
        {   "when": { "east": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "y": 90, "uvlock": true }
            ]
        },
        {   "when": { "west": false,  "east": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_horizontal", "weight": 2, "y": 270, "uvlock": true }
            ]
        },
        {   "when": { "west": false,  "east": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_horizontal", "weight": 2, "y": 90, "uvlock": true }
            ]
        },
        {   "when": { "north": false,  "south": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_horizontal", "weight": 2, "uvlock": true }
            ]
        },
        {   "when": { "north": false,  "south": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_horizontal", "weight": 2, "y": 180, "uvlock": true }
            ]
        },
        {   "when": { "east": false,  "south": false, "west": false},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "y": 180, "uvlock": true }
            ]
        },
        {   "when": { "north": false,  "east": false, "west": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "uvlock": true }
            ]
        },
        {   "when": { "north": false,  "south": false, "west": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "y": 270, "uvlock": true }
            ]
        },
        {   "when": { "north": false,  "south": false, "east": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "y": 90, "uvlock": true }
            ]
        },
        {   "when": { "up": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "x": 270, "uvlock": true }
            ]
        },
        {   "when": { "down": false },
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "x": 90, "uvlock": true }
            ]
        },
        {   "when": { "OR":[{"down": true}, {"up": true}]},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_vertical", "weight": 2, "uvlock": true }
            ]
        },
        {   "when": { "OR":[{"down": true}, {"up": true}]},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_vertical", "weight": 2, "y": 90, "uvlock": true }
            ]
        },
        {   "when": { "OR":[{"down": true}, {"up": true}]},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_vertical", "weight": 2, "y": 180, "uvlock": true }
            ]
        },
        {   "when": { "OR":[{"down": true}, {"up": true}]},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_vertical", "weight": 2, "y": 270, "uvlock": true }
            ]
        },
        {   "when": {"OR": [ {"west": true}, {"east": true} ]},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "y": 270, "uvlock": true }
            ]
        },
        {   "when": {"OR": [ {"west": true}, {"east": true} ]},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "y": 90, "uvlock": true }
            ]
        },
        {   "when": {"OR": [ {"north": true}, {"south": true} ]},
            "apply": [
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "uvlock": true },
                { "model": "woods_mod:block/spruce_flexible_branch_noside_top", "weight": 2, "x": 180, "uvlock": true }
            ]
        }
    ]
}